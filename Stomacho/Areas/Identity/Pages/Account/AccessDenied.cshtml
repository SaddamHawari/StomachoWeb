@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header class="text-center my-4">
    <h1 class="text-danger fw-bold"><i class="la la-exclamation-triangle"></i> @ViewData["Title"]</h1>
    <p class="text-muted fs-5">Sorry, you do not have permission to access this resource.</p>
</header>