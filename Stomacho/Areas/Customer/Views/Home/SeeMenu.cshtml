@model SeeMenuViewModel

<!-- ================================
START BREADCRUMB AREA
================================= -->
<section class="breadcrumb-area py-0" style="background: url('@Url.Content(Model.Restaurant.ImageUrl.Replace("\\", "/"))') no-repeat center center;
           background-size: cover;
           min-height: 500px;">
    <div class="breadcrumb-wrap">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-btn">
                        <div class="btn-box">
                            <a class="theme-btn" data-fancybox="video" data-src="https://www.youtube.com/watch?v=5u1WISBbo5I"
                            data-speed="700">
                                <i class="la la-video-camera mr-2"></i>Video
                            </a>
                            <a class="theme-btn" data-src="@Model.Restaurant.ImageUrl"
                            data-fancybox="gallery"
                            data-caption="Showing image - 01"
                            data-speed="700">
                                <i class="la la-photo mr-2"></i>More Photos
                            </a>
                        </div>
                    </div><!-- end breadcrumb-btn -->
                </div><!-- end col-lg-12 -->
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end breadcrumb-wrap -->
</section><!-- end breadcrumb-area -->
<!-- ================================
END BREADCRUMB AREA
================================= -->
<!-- ================================
START TOUR DETAIL AREA
================================= -->
